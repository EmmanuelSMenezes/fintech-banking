# FinTech Banking - Admin Dashboard
# Copie este arquivo para .env.local e configure os valores

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:5036
NEXT_PUBLIC_API_INTERNAL=http://localhost:5036

# Auth Configuration
NEXT_PUBLIC_AUTH_PROVIDER=jwt
NEXT_PUBLIC_JWT_SECRET=your-secret-key-here

# App Configuration
NEXT_PUBLIC_APP_NAME=FinTech Admin Dashboard
NEXT_PUBLIC_APP_VERSION=1.0.0
NEXT_PUBLIC_APP_DESCRIPTION=Painel administrativo para gerenciamento de clientes e transações

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_LOGGING=true

# Ports
NEXT_PUBLIC_ADMIN_PORT=3000
NEXT_PUBLIC_BANKING_PORT=3001

# Database (se necessário)
DATABASE_URL=postgresql://user:password@localhost:5432/fintech_admin

# Logging
LOG_LEVEL=info

